@model List<ISP_Biblioteka.Models.Order>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Užsakymų istorija</title>
</head>
<body>
    <br>
    <br>
    <h1 style="text-align:left">Užsakymų istorija</h1>
    <table class="table table-striped table-hover">
        <thead class="thead-dark">
            <tr>
                <th scope="col">ID</th>
                <th>Užsakymo data</th>
            </tr>
        </thead>
        <tbody>
            @foreach (ISP_Biblioteka.Models.Order order in Model)
            {
                <tr>
                    <th>@order.ID</th>
                    <th>@order.Borrow_date</th>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
