@model IEnumerable<ISP_Biblioteka.Models.User>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            <th>Name</th>
            <th>ID</th>
        </tr>
        @foreach (ISP_Biblioteka.Models.User details in Model)
        {
            System.Diagnostics.Debug.WriteLine(@details.ImagePath);
            <tr>
                <td>@details.Name</td>
                <td>@details.Id</td>
                <td><img src="@Url.Content(@details.ImagePath)" width="50" height="50"></td>
            </tr>
        }
    </table>
</body>
</html>
